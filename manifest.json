{
    "manifest_version": 3,
    "name": "ClaritySEO",
    "version": "1.0",
    "description": "A simple SEO checker Chrome extension.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "/Scripts/script.js"
    }
}